<Application x:Class="Presentation.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Presentation"
             StartupUri="Views/Principal.xaml"
             Startup="Application_Startup"
             LoadCompleted="Application_LoadCompleted">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Presentation;component/Estilos/Cor/Cores.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Presentation;component/Estilos/TextBlockStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Presentation;component/Estilos/ScrollBarStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Presentation;component/Estilos/ButtonStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Presentation;component/Estilos/TextBoxStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Presentation;component/Estilos/ComboBoxStyle.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/Presentation;component/Estilos/CredencialListBoxStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
